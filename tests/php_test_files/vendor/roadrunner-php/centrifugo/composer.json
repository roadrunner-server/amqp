    "scripts": {
        "tests": "phpunit",
        "psalm": "psalm --no-cache",
        "compile": "protoc --proto_path=resources/proto/v1 --php_out=generated resources/proto/v1/proxy.proto resources/proto/v1/service.proto"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
